{
  "name": "sdusd-gotw",
  "version": "0.0.0",
  "description": "Broadcast graphics for SDUSD live sports events.",
  "author": {
    "name": "Samuel Sharp",
    "email": "samuelsharp4@gmail.com"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "assetCategories": [
      {
        "name": "logos",
        "title": "Team Logos",
        "allowedTypes": [
          "png"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "panel",
        "title": "âœ¦ Main Panel",
        "file": "panel.html",
        "fullbleed": true
      },
      {
        "name": "config",
        "title": "Setup",
        "file": "config.html",
        "fullbleed": true
      },
      {
        "name": "help",
        "title": "Help",
        "file": "help.html",
        "fullbleed": true
      }
    ],
    "soundCues": [
      {
        "name": "bg-music",
        "channels": 1,
        "defaultVolume": 80,
        "assignable": false,
        "defaultFile": "sound/music.mp3"
      }
    ],
    "graphics": [
      {
        "file": "gotw-core.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "jquery": "^3.7.1"
  }
}